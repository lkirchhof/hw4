<div class=container-fluid>
  <h1>Login</h1>

  <%= form_with :url => "/sessions" do |form| %>
    <p>
      <%= form.label "E-mail" %>
      <%= form.text_field "email" %>
    </p>
  <p>
      <%= form.label "Password" %>
      <%= form.password_field "password" %>
  </p>
    <p>
      <%= form.submit "Login", class: "btn btn-outline-dark" %>
    </p>
  <% end %>
  </div>